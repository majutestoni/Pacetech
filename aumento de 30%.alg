algoritmo "aumento de salario"

var
        salario1: real
        salario2: real
inicio
           escreval("Informe seu salario para receber um aumento!")
           leia(salario1)
           salario2 <- salario1 * 1.3
           escreval("Parabens pelo aumento de 30%!")
           escreval("R$", salario2)
       
fimalgoritmo

